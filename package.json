{
  "name": "portfolio-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy:s3": "npm run generate && aws s3 cp ./dist s3://dco-bucket --recursive",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "fix:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:formatting": "npx prettier --config ./prettier.config.js **/*.* --check",
    "fix:formatting": "npx prettier --config ./prettier.config.js **/*.* --write"
  },
  "engines": {
    "node": ">=0.10.3 <15",
    "npm": ">=6"
  },
  "dependencies": {
    "@nuxt/http": "^0.6.4",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "core-js": "^3.14.0",
    "express": "^4.17.1",
    "hamburgers": "^1.1.3",
    "hover.css": "^2.3.2",
    "nuxt": "^2.15.6",
    "smooth-scroll-into-view-if-needed": "^1.1.32"
  },
  "devDependencies": {
    "@mdi/js": "^5.9.55",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.3",
    "altheajs-prettier-config": "^1.1.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "prettier": "^2.3.1",
    "sass": "~1.32.12",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0"
  }
}
